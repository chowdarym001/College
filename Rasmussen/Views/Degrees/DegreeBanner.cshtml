@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model Rasmussen.Models.GenericContentBlockModel

<div class="banner-full banner-full--imageless">
    <div class="container">
        <div class="banner-text text--dark-on-light">
            <h1 class="banner-text__headline banner-text__headline--lvl-1">@Html.Raw(Model.Title)</h1>
            <h2 class="banner-text__headline banner-text__headline--lvl-2">@Html.Raw(Model.Body)</h2>
        </div>
    </div>
</div>
